#include <iostream>#include <iostream>
using namespace std;

int main()
{
    int x, y;
    cout << "Enter the number: ";
    cin >> x;
    cout << "Enter the number: ";
    cin >> y;
    bool flag = true;
    while (y != 0)
    {
        if (y <= x)
        {
            flag = false;
        }
        x = y;
        cout << "Enter the number: ";
        cin >> y;
    }
    if (flag == false)
    {
        cout << "false";
    }
    else
    {
       cout << "true";
    }


    return 0;
}
	

}
