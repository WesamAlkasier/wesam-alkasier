#include <iostream>
using namespace std;

int main()
{
	int n, a;
	bool flag = true;
	cout << "amount of elements: ";
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		cin >> a;
		if (a < 0)
		{
			cout << "Negative first";
			flag = false;
			break;
		}
		else if (a > 0)
		{
			cout << "Positive first";
			flag = false;
			break;
		}
	}
	if (flag == true)
	{
		cout << "all elements zero";
	}
	return 0;
}
