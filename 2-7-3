#include <iostream>
using namespace std;

int main()
{
	int n, x;
	while (true)
	{
		cout << "Enter N\n";
		cin >> n;
		for (int i = 0; i <n; i++)
		{
			if (i == 0 || i == n - 1)
			{
				for (int k = 0; k < n; k++)
				{
					cout << "*";
				}
			}
			else {
				cout << "*";
				for (int k = 0; k < n-2; k++)
				{
					cout << " ";
				}
				cout << "*";
			}
			
			
			cout << endl;
			
		}
		cout << endl << endl;

			
	}
	
}
